<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="custom_snippets_appeul_travel" inherit_id="website.snippets" name="Custom Snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">Appeul Snippets</div>
                <div class="o_panel_body">
                    <t t-snippet="theme_clean.appaeul_explore_hotels_travel">
                        <keywords>hotels</keywords>
                    </t>

                </div>
            </div>
        </xpath>
    </template>


</odoo>